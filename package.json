{
  "name": "antd-ui-b",
  "version": "2.0.10",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "seceng",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:TerrySlack/antd-ui-b.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "antd-ui-b"
  ],
  "scripts": {
    "prestart": "npm run prep",
    "build-": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "test": "jest",
    "lint": "eslint --ext 'mjs,js,jsx,ts,tsx' --max-warnings=0 --fix",
    "lint-staged": "lint-staged",
    "prepare-husky": "husky install",
    "prep": "npm install && npm run  prepare-husky",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint-staged",
    "storybook": "start-storybook -p 6006 -s public --ci",
    "storybook:heroku": "npx http-server build -p $PORT",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "heroku-postbuild": "npm run build && npm run build:storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged  && git add --all"
    }
  },
  "lint-staged": {
    "*.(js|ts|jsx|tsx)": [
      "eslint --max-warnings=0 --fix --ignore-pattern \"!/.storybook\"",
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.22.8",
    "deep-equal": "^2.0.5",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "react": ">=16",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/types": "^29.0.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.1.21",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.8",
    "babel-jest": "^29.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.25.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cypress": "^10.7.0",
    "cypress-pipe": "^2.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "install": "^0.13.0",
    "jest": "^28.1.3",
    "jest-canvas-mock": "^2.4.0",
    "jest-cli": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "8",
    "postcss-easings": "^3.0.1",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1",
    "webpack-merge": "^5.8.0",
    "worker-loader": "^3.0.8"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ],
      [
        "@babel/preset-react"
      ]
    ],
    "plugins": [
      "syntax-dynamic-import",
      "transform-class-properties",
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "testEnvironment": "jsdom",
    "coverageDirectory": "dist/test-results",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "Assets/(.*)": "<rootDir>/src/shared/assets/$1",
      "Components/(.*)": "<rootDir>/src/shared/components/$1",
      "Containers/(.*)": "<rootDir>/src/shared/containers/$1",
      "HOC/(.*)": "<rootDir>/src/shared/hoc/$1",
      "Hooks/(.*)": "<rootDir>/src/shared/hooks/$1",
      "Utils/(.*)": "<rootDir>/src/shared/utils/$1",
      "Context/(.*)": "<rootDir>/src/shared/context/$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 4,
        "functions": 4,
        "lines": 4,
        "statements": 4
      }
    }
  }
}
